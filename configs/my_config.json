{
    "exp": {
        "name": "vgg16_transfer"
    },
    "data": {
        "test_size":0.3,
        "IMAGE_SIZE": 96,
        "COLOR_CHANNELS": 1,
        "NUM_LANDMARKS": 30
    },
    "model":{
        "learning_rate": 0.001,
        "optimizer": "adam"
    },
    "trainer":{
        "num_epochs": 20,
        "batch_size": 64,
        "verbose_training": true
    },
    "callbacks":{
        "checkpoint_monitor": "val_loss",
        "checkpoint_mode": "min",
        "checkpoint_save_best_only": true,
        "checkpoint_save_weights_only": true,
        "checkpoint_verbose": true,
        "tensorboard_write_graph": true
    }
}
